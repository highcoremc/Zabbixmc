@import 'shared/variables.styl'
@import 'shared/reset.styl'
@import 'shared/fonts.styl'

body
  background #f5f5fd
  
.disable-highlight
  -webkit-touch-callout none
  -webkit-user-select none
  -khtml-user-select none
  -moz-user-select none
  -ms-user-select none
  user-select none

@media (min-width: 1600px)
 .container-xl,
 .container-lg,
 .container-md,
 .container-sm,
 .container {
   max-width: 1300px;
 }
